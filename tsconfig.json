{
  "extends": "expo/tsconfig.base",
  
    "compilerOptions": {
      "target": "esnext",
      "jsx": "react",
      "module": "commonjs",
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,  // Add this line
      "allowSyntheticDefaultImports": true,  // Add this line
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": ".",
      "paths": {
        "*": ["node_modules/*"]
      }
    },
    "include": ["src"],
    "exclude": ["node_modules"]
  }

